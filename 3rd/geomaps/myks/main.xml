<?xml version="1.0"?>
<!DOCTYPE myks PUBLIC  "-//YKS//MYKS" "http://rsrcs.exyks.org/dtds/myks.dtd">
<myks>
  <table name="ks_geomaps_area_list">
    <fields>
      <field type="area_id"/>
      <field type="area_key"/>
      <field type="title" name="area_name"/>
    </fields>
  </table>
  <mykse type="area_id" birth="ks_geomaps_area_list">
    <string length="50"/>
  </mykse>
  <mykse type="area_key">
    <string length="20"/>
  </mykse>
  <table name="ks_users_geomaps_list">
    <fields>
      <field type="map_id"/>
      <field type="title" name="map_name"/>
      <field type="user_id"/>
    </fields>
  </table>
  <mykse type="map_id" birth="ks_users_geomaps_list">
    <int/>
  </mykse>
  <table name="ks_users_geomaps_area">
    <fields>
      <field type="map_id"/>
      <field type="area_id"/>
      <field type="user_id"/>
    </fields>
  </table>
</myks>

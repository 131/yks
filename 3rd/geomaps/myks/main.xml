<?xml version="1.0"?>
<!DOCTYPE myks PUBLIC  "-//YKS//MYKS" "http://rsrcs.exyks.org/dtds/myks.dtd">
<myks>
  <mykse type="area_id">
    <string length="50"/>
  </mykse>
  <table name="ks_users_geomaps_list">
    <fields>
      <field type="map_id"/>
      <field type="title" name="map_name"/>
      <field type="user_id"/>
      <field type="uri" name="map_path"/>
    </fields>
  </table>
  <mykse type="map_id" birth="ks_users_geomaps_list">
    <int/>
  </mykse>
  <table name="ks_users_geomaps_area">
    <fields>
      <field type="map_id"/>
      <field type="area_id"/>
      <field type="user_id"/>
    </fields>
  </table>
</myks>

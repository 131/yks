<?xml version="1.0"?>
<!DOCTYPE myks PUBLIC "-//YKS//MYKS">
<myks>

  <procedure name="coalesce_first" type="int" volatility="IMMUTABLE">
    <param type="int"/>
    <param type="int"/>
    <def>
BEGIN
    return COALESCE($1, $2);
END;
</def>
  </procedure>
  <procedure name="coalesce_first" type="string" volatility="IMMUTABLE">
    <param type="string"/>
    <param type="string"/>
    <def>
BEGIN
    return COALESCE($1, $2);
END;
</def>
  </procedure>
  <procedure name="coalesce_first" type="text" volatility="IMMUTABLE">
    <param type="text"/>
    <param type="text"/>
    <def>
BEGIN
    return COALESCE($1, $2);
END;
</def>
  </procedure>
</myks>

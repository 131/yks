Aggregates arent managed (yet)
Please create
CREATE LANGUAGE 'plpgsql' HANDLER plpgsql_call_handler LANCOMPILER 'PL/pgSQL'


CREATE AGGREGATE "public"."concat_comma" ( BASETYPE = "text",    SFUNC = "public"."concat_comma", STYPE = "text");


CREATE AGGREGATE first(int) (    sfunc = coalesce_first,     stype = int );
CREATE AGGREGATE first(text) (    sfunc = coalesce_first,     stype = text );
CREATE AGGREGATE first(varchar) (    sfunc = coalesce_first,     stype = varchar );
CREATE AGGREGATE first(boolean) (    sfunc = coalesce_first,     stype = boolean );



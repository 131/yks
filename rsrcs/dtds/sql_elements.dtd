<!ENTITY % Num "CDATA">

<!ENTITY % IntSizes "(mini|small|int|big|giga|float|decimal)">
<!ENTITY % Events       "(insert|update|delete)">
<!ENTITY % EventsTimer  "(before|after)">


<!ENTITY % null
  "null  (null|not_null)  #IMPLIED"
>


<!ENTITY % block
 "block CDATA	 #IMPLIED"
>
<!ELEMENT int (#PCDATA)>
<!ATTLIST int
  %null;
  signed	(signed|unsigned)	'unsigned'
  size		%IntSizes;	'small'
>

<!ELEMENT enum (val+)>
<!ELEMENT val (#PCDATA)>
<!ATTLIST enum
  %null;
  set	(set)	#IMPLIED
>



<!ELEMENT string  (#PCDATA)>
<!ATTLIST string
  %null;
  length	CDATA	"12"
>

<!ELEMENT text (CDATA)*>
<!ATTLIST text
  %null;
>

<!ELEMENT json (CDATA)*>

<!ELEMENT mykse (mykse|text|string|enum|int|json)?>

<!ATTLIST mykse
  type  CDATA	#REQUIRED
  birth	CDATA	#IMPLIED
  default CDATA	#IMPLIED
  %null;
  %block;
>

<!ELEMENT grant EMPTY>
<!ATTLIST grant
  actions CDATA #IMPLIED
  to CDATA #IMPLIED
>



